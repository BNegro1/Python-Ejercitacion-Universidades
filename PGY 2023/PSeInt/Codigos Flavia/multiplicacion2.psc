Algoritmo multiplicacion2
	
	Definir i, num1, num2, suma Como Entero;
	suma=0;
	Escribir "Ingresar primer número a multiplicar";
	Leer num1;
	Escribir "Ingresar segundo número a multiplicar";
	Leer num2;
	Mientras i<num2 Hacer
		suma=num1+suma;
		i=i+1
	FinMientras
	
	Escribir num1, "*", num2, "=", suma;

	
FinAlgoritmo
